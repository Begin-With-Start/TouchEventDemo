<?xml version="1.0" encoding="utf-8"?>
<com.minifly.demo.touchevent.ViewEvent.ConstraintLayoutMy xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:id="@+id/event_demo_constraintlayoutid"
    tools:context="com.minifly.demo.touchevent.MainActivity">

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Hello World!"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginLeft="-2dp"
        android:layout_marginTop="3dp" />


        <com.minifly.demo.touchevent.ViewEvent.TouchEventView
            android:layout_width="200dp"
            android:layout_height="200dp"
            android:id="@+id/event_demo_viewid"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            />


</com.minifly.demo.touchevent.ViewEvent.ConstraintLayoutMy>
